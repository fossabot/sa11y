{
    "name": "@sa11y/browser-lib",
    "version": "0.1.0-alpha",
    "description": "Sa11y libraries bundled to be injected into browser and executed from integration testing workflows",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/salesforce/sa11y/tree/master/packages/browser-lib#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/salesforce/sa11y.git",
        "directory": "packages/browser-lib"
    },
    "keywords": [
        "accessibility",
        "automated test",
        "integration test",
        "browser",
        "Webdriver",
        "axe"
    ],
    "browser": "dist/sa11y.min.js",
    "files": [
        "dist/**/*.js"
    ],
    "scripts": {
        "build": "rollup -c",
        "build:watch": "yarn build --watch",
        "test": "wdio run ../../wdio.conf.js",
        "test:debug": "DEBUG=true yarn test",
        "test:watch": "yarn test --watch"
    },
    "bugs": {
        "url": "https://github.com/salesforce/sa11y/issues"
    },
    "dependencies": {
        "@sa11y/assert": "0.1.3-beta.0",
        "@sa11y/format": "0.2.1-beta.0",
        "@sa11y/preset-rules": "0.2.0-beta.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^14.0.0",
        "@rollup/plugin-node-resolve": "^8.4.0",
        "@wdio/cli": "^6.2.0",
        "@wdio/local-runner": "^6.2.0",
        "@wdio/mocha-framework": "^6.2.0",
        "@wdio/spec-reporter": "^6.1.23",
        "chromedriver": "^83.0.1",
        "rollup": "^2.21.0",
        "rollup-plugin-progress": "^1.1.2",
        "rollup-plugin-sizes": "^1.0.2",
        "rollup-plugin-terser": "^6.1.0",
        "rollup-plugin-typescript2": "^0.27.1",
        "ts-node": "^8.10.2",
        "wdio-chromedriver-service": "^6.0.3"
    }
}
